<% if (errors && Array.isArray(errors)) { %>
  <% const err=errors.find(e=> e.path === field); %>
    <% if (err) { %>
      <div class="p-2 mb-4 text-sm text-red-800 rounded-lg bg-red-50 dark:bg-gray-800 dark:text-red-400" role="alert">
        <%= err.msg %>
      </div>
      <% } %>
        <% } %>
